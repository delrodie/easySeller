<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimal-ui">
        <title>{% block title %}EasySeller{% endblock %}</title>
        {% block stylesheets %}
        <link href="{{ asset("ressources/vendor/css/bootstrap.min.css")}}" rel="stylesheet" type="text/css">
        <link href="{{ asset("ressources/vendor/css/icons.css")}}" rel="stylesheet" type="text/css">
        <link href="{{ asset("ressources/vendor/css/style.css")}}" rel="stylesheet" type="text/css">
        <link href="{{ asset("ressources/css/easyseller.css")}}" rel="stylesheet" type="text/css">
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('ressources/images/favicon.png') }}" />
    </head>
    <body class="fixed-left">
      <div class="wrapper">
        <!-- Top Bar Start -->
           <div class="topbar">
               <!-- LOGO -->
               <div class="topbar-left">
                   <div class="text-center">
                     <a href="#" class="logo">
                          <span class="logo-e">e</span><span class="logo-asy">asy</span><span class="logo-s">s</span><span class="logo-asy">eller</span>
                      </a>
                      <a href="index.html" class="logo-sm"><span>ES</span></a>
                   </div>
               </div>
               <!-- Button mobile view to collapse sidebar menu -->
               <div class="navbar navbar-default" role="navigation">
                   <div class="container">
                       <div class="">
                           <div class="pull-left">
                               <button type="button" class="button-menu-mobile open-left waves-effect waves-light">
                                   <i class="ion-navicon"></i>
                               </button>
                               <span class="clearfix"></span>
                           </div>

                           <ul class="nav navbar-nav navbar-right pull-right">

                            {{ render(url('fseur_bon_approvisonnement'))}}

                               <li class="dropdown hidden-xs">
                                   <a href="#" data-target="#" class="dropdown-toggle waves-effect waves-light notification-icon-box" data-toggle="dropdown" aria-expanded="true">
                                       <i class="fa fa-shopping-cart"></i> <span class="badge badge-xs badge-danger">17</span>
                                   </a>
                                   <ul class="dropdown-menu dropdown-menu-lg">
                                       <li class="text-center notifi-title">Nom client <span class="badge badge-xs badge-success">3</span></li>
                                       <li class="list-group">
                                          <!-- list item-->
                                          <a href="javascript:void(0);" class="list-group-item">
                                             <div class="media">
                                                 <i class="fa fa-diamond text-danger noti-sm-icon"></i>
                                                 <div class="noti-content">
                                                     <div class="media-heading">Your order is placed</div>
                                                     <p class="m-0">
                                                         <small>Dummy text of the printing and typesetting industry.</small>
                                                     </p>
                                                 </div>
                                             </div>
                                          </a>
                                          <!-- list item-->
                                           <a href="javascript:void(0);" class="list-group-item">
                                             <div class="media">
                                                 <i class="fa  fa-envelope-o text-primary noti-sm-icon"></i>
                                                 <div class="noti-content">
                                                     <div class="media-heading">New Message received</div>
                                                     <p class="m-0">
                                                         <small>You have 87 unread messages</small>
                                                     </p>
                                                 </div>
                                             </div>
                                           </a>
                                           <!-- list item-->
                                           <a href="javascript:void(0);" class="list-group-item">
                                               <i class="fa fa-fighter-jet text-warning noti-sm-icon"></i>
                                               <div class="noti-content">
                                                   <div class="media-heading">Your item is shipped.</div>
                                                   <p class="m-0">
                                                       <small>It is a long established fact that a reader will</small>
                                                   </p>
                                               </div>
                                           </a>
                                          <!-- last list item -->
                                           <a href="javascript:void(0);" class="list-group-item">
                                             <small class="text-primary">Valider la facture</small>
                                           </a>
                                       </li>
                                   </ul>
                               </li>
                               <li class="hidden-xs">
                                   <a href="#" id="btn-fullscreen" class="waves-effect waves-light notification-icon-box"><i class="ion-qr-scanner"></i></a>
                               </li>
                               <li class="dropdown">
                                   <a href="#" class="dropdown-toggle profile waves-effect waves-light" data-toggle="dropdown" aria-expanded="true">
                                       <img src="{{ asset("ressources/images/avatar.png")}}" alt="user-img" class="img-circle">
                                       <span class="profile-username">
                                         {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
                                            {{ 'layout.user'|trans({'%username%': app.user.username}, 'FOSUserBundle') }}
                                          {% endif %}
                                          <span class="caret"></span>
                                       </span>
                                   </a>
                                   <ul class="dropdown-menu">
                                     <li><a href="{{ path('fos_user_change_password')}}">Changer le mot de passe </a></li>
                                      <li class="divider"></li>
                                      <li><a href="{{ path('fos_user_security_logout')}}"> Deconnexion</a></li>
                                   </ul>
                               </li>
                           </ul>
                       </div>
                       <!--/.nav-collapse -->
                   </div>
               </div>
           </div>
           <!-- Top Bar End -->

           <!-- ========== Left Sidebar Start ========== -->

            <div class="left side-menu">
                <div class="sidebar-inner slimscrollleft">

                    <form class="sidebar-search">
                        <div class="">
                            <input type="text" class="form-control search-bar" placeholder="Recherche...">
                        </div>
                        <button type="submit" class="btn btn-search"><i class="fa fa-search"></i></button>
                    </form>

                    <div id="sidebar-menu">
                        <ul>
                            <li>
                                <a href="{{ path('homepage') }}" class="waves-effect"><i class="ion-ios7-analytics"></i><span> Tableau de bord </span></a>
                            </li>

                            <li class="menu-title">CAISSE</li>

                            <li class="has_sub">
                                <a href="javascript:void(0);" class="waves-effect"><i class="fa fa-calculator"></i><span> Facturation </span><span class="pull-right"><i class="mdi mdi-chevron-right"></i></span></a>
                                <ul class="list-unstyled">
                                    <li><a href="#">Vente</a></li>
                                    <li><a href="#">Pro-forma</a></li>
                                    <li><a href="#">Annuler une facture de vente</a></li>
                                </ul>
                            </li>

                            <li class="has_sub">
                                <a href="javascript:void(0);" class="waves-effect"><i class="mdi mdi-account-plus"></i> <span> Client </span> <span class="pull-right"><i class="mdi mdi-chevron-right"></i></span></a>
                                <ul class="list-unstyled">
                                    <li><a href="#">Client</a></li>
                                </ul>
                            </li>

                            <li class="has_sub">
                                <a href="javascript:void(0);" class="waves-effect"><i class="fa fa-money"></i><span> Caisse </span><span class="pull-right"><i class="mdi mdi-chevron-right"></i></span></a>
                                <ul class="list-unstyled">
                                    <li><a href="#">Arrêté de caisse</a></li>
                                    <li><a href="#">Encaisser recette </a></li>
                                    <li><a href="{{ path('bon_reception_fournisseur')}}">Achats </a></li>
                                </ul>
                            </li>

                            <li class="menu-title">GESTIONNAIRE</li>

                            <li class="has_sub">
                                <a href="javascript:void(0);" class="waves-effect"><i class="fa fa-user-secret"></i> <span> Fournisseur </span> <span class="pull-right"><i class="mdi mdi-chevron-right"></i></span></a>
                                <ul class="list-unstyled">
                                    <li><a href="{{ path('fournisseur_index')}}">Fournisseur</a></li>
                                    <li><a href="#">Bon de commande</a></li>
                                    <li><a href="{{ path('bon_reception_fournisseur')}}">Bon de reception</a></li>
                                </ul>
                            </li>

                            <li class="has_sub">
                                <a href="javascript:void(0);" class="waves-effect"><i class="ion-cube"></i><span> Produits </span><span class="pull-right"><i class="mdi mdi-chevron-right"></i></span></a>
                                <ul class="list-unstyled">
                                    <li><a href="{{ path('categorie_index') }}">Categorie</a></li>
                                    <li><a href="{{ path('produit_index')}}">Article</a></li>
                                    <li><a href="{{ path('approvisionnement_index')}}">Approvisionnement</a></li>
                                    <li><a href="#">Inventaire</a></li>
                                </ul>
                            </li>

                            <li class="menu-title">ADMINISTRATEUR</li>

                            <li class="has_sub">
                                <a href="javascript:void(0);" class="waves-effect"><i class="fa fa-cogs"></i><span> Paramètre </span><span class="pull-right"><i class="mdi mdi-chevron-right"></i></span></a>
                                <ul class="list-unstyled">
                                    <li><a href="#"> Personnel</a></li>
                                    <li><a href="{{ path('admin_user_index') }}"> Utilisateur</a></li>
                                </ul>
                            </li>

                        </ul>
                    </div>
                    <div class="clearfix"></div>
                </div> <!-- end sidebarinner -->
            </div>
            <!-- Left Sidebar End -->

            <!-- Start right Content here -->
            <div class="content-page">
              <!-- Start content -->
              <div class="content">
                {% block body %}{% endblock %}
              </div>
              <footer class="footer">
                     © 2017 EasySeller - By Eden'Art.
                </footer>
            </div>
      </div>

        {% block javascripts %}
        <script src="{{ asset("ressources/vendor/js/jquery.min.js")}}"></script>
        <script src="{{ asset("ressources/vendor/js/bootstrap.min.js")}}"></script>
        <script src="{{ asset("ressources/vendor/js/modernizr.min.js")}}"></script>
        <script src="{{ asset("ressources/vendor/js/detect.js")}}"></script>
        <script src="{{ asset("ressources/vendor/js/fastclick.js")}}"></script>
        <script src="{{ asset("ressources/vendor/js/jquery.slimscroll.js")}}"></script>
        <script src="{{ asset("ressources/vendor/js/jquery.blockUI.js")}}"></script>
        <script src="{{ asset("ressources/vendor/js/waves.js")}}"></script>
        <script src="{{ asset("ressources/vendor/js/wow.min.js")}}"></script>
        <script src="{{ asset("ressources/vendor/js/jquery.nicescroll.js")}}"></script>
        <script src="{{ asset("ressources/vendor/js/jquery.scrollTo.min.js")}}"></script>
        {% endblock %}
    </body>
</html>
